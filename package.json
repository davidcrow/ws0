{
  "devDependencies": {
    "@11ty/eleventy": "^3.1.1",
    "@tailwindcss/postcss": "^4.1.9",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.5",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.9",
    "tailwindcss-cli": "^0.1.2",
    "wrangler": "^4.19.2"
  },
  "scripts": {
    "build:css": "npx tailwindcss-cli -i ./src/css/tailwind.css -o ./_site/style.css --minify",
    "build": "npm run build:css && eleventy",
    "dev": "npx tailwindcss-cli -i ./src/css/tailwind.css -o ./_site/style.css --watch & eleventy --serve",
    "deploy": "wrangler deploy"
  }
}
